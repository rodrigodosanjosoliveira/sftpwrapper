<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Rodrigo Oliveira</Authors>
    <Company>Rodrigo Oliveira</Company>
    <Product>SftpWrapper</Product>
    <Description>Wrapper para utilização do SFTP do pacote SSH.NET</Description>
    <PackageProjectUrl>https://github.com/rodrigodosanjosoliveira/SftpWrapper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/rodrigodosanjosoliveira/SftpWrapper</RepositoryUrl>
    <NeutralLanguage></NeutralLanguage>
    <Version>1.0.4</Version>
    <AssemblyVersion>1.0.0.3</AssemblyVersion>
    <FileVersion>1.0.0.3</FileVersion>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageLicenseUrl>https://github.com/rodrigodosanjosoliveira/SftpWrapper</PackageLicenseUrl>
    <Copyright>Copyright © 2017 Rodrigo Oliveira</Copyright>
    <PackageId>SftpWrapper.Sdk</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Services\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="SSH.NET" Version="2016.1.0" />
  </ItemGroup>
</Project>
